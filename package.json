{
  "name": "1.1",
  "version": "1.0.0",
  "description": "Template for Ts, Tailwind, Jest and NodeJS",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run build:css\" \"npm run build:ts\" \"npm run start:server\"",
    "test": "jest",
    "build:css": "tailwindcss -i ./tailwind.css -o ./public/output.css --watch",
    "build:ts": "tsc --watch",
    "build": "npm run build:css && npm run build:ts",
    "start:cli": "node dist/cli.js",
    "start:server": "node --watch dist/server.js"
  },
  "keywords": [],
  "author": "Adan Reh",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node-snap7": "^0.2.24",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "node-snap7": "^1.0.8",
    "telegraf": "^4.16.3"
  }
}
